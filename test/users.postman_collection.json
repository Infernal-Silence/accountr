			"name": "Регистрация",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "3fe48029-2697-47e1-af72-32b80abea2e0",
						"exec": [
							"pm.test(\"Successful POST request\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(pm.response.code).to.eql(201);\r",
							"    pm.expect(jsonData).to.include({\r",
							"        id:1,\r",
							"        email: \"user1@gmail.com\",\r",
							"        first_name: \"user1\",\r",
							"        last_name: \"user1\"\r",
							"    });\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n\t\"email\": \"user1@gmail.com\",\n\t\"password\": \"user1\",\n\t\"first_name\": \"user1\",\n\t\"last_name\": \"user1\"\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "http://127.0.0.1:5000/users",
					"protocol": "http",
					"host": [
						"127",
						"0",
						"0",
						"1"
					],
					"port": "5000",
					"path": [
						"users"
					]
				}
			},
